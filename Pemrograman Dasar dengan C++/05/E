#include <iostream>
using namespace std;

int main() {
    int N = 10;

    for (int i = 1; i <= N; i++) {
        // cetak i buah "*"
        for (int j = 0; j < i; j++) {
            cout << "*";
        }

        // sisanya, cetak "."
        for (int k = N; k > i; k--) {
            cout << ".";
        }

        cout << endl;
    }
}
